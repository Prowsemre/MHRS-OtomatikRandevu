# MHRS Bot Ayarları
# Bu dosyayı .env olarak kaydedin ve kendi bilgilerinizle doldurun

# MHRS Giriş Bilgileri
MHRS_TC=12345678901
MHRS_PASSWORD=your_password

# Lokasyon ID'leri
# -1 = Farketmez
MHRS_PROVINCE_ID=70
MHRS_DISTRICT_ID=1439
MHRS_CLINIC_ID=196
MHRS_HOSPITAL_ID=280
MHRS_PLACE_ID=-1
MHRS_DOCTOR_ID=40143

# Tarih Ayarları
# Format: GG-AA-YYYY (gün-ay-yıl)
# Boş bırakılırsa bugünden başlar
MHRS_START_DATE=24-08-2025
MHRS_END_DATE=29-08-2025

# Telegram Bot Bildirimleri
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=
TELEGRAM_NOTIFY_FREQUENCY=10

# E-posta Bildirimleri (opsiyonel)
EMAIL_SMTP_HOST=
EMAIL_USERNAME=
EMAIL_PASSWORD=
EMAIL_TO=

# SMS Bildirimleri (opsiyonel)
